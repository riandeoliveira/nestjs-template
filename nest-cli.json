{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "compilerOptions": {
    "assets": [
      {
        "include": "./**/*.hbs",
        "outDir": "dist/src",
        "watchAssets": true
      }
    ],
    "deleteOutDir": true
  },
  "sourceRoot": "src"
}
